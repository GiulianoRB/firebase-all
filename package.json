{
  "version": "0.1.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": ["firebase", "auth", "firestore", "crud firestore"],
  "repository": {
    "type": "git",
    "url": "https://github.com/GiulianoRB/firebase-all.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "firebase-all",
  "author": "Giuliano Rivera",
  "module": "dist/firebase-all.esm.js",
  "size-limit": [
    {
      "path": "dist/firebase-all.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/firebase-all.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "husky": "^9.1.6",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "firebase": "^11.0.1"
  }
}
